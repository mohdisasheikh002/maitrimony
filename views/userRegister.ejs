<%- include('./partials/header')%>
<!--  -->
<div
  class="holder formHolder w-full flex items-center justify-center relative bg-white pb-[4.5vmax] max-sm:px-[2.5vmax]"
>
  <div
    class="form p-[4vmax] pt-[3.5vmax] rounded-md mt-[10vmax] z-10 bg-[#cf2e2e] max-sm:w-full max-sm:p-[2.5vmax] max-sm:mt-[12.5vmax] max-sm:pt-[2vmax] max-sm:pb-[3.5vmax]"
  >
    <h1
      class="text-[2vmax] text-white text-center tracking-tight max-sm:text-[3.5vmax]"
    >
      Registration Form
    </h1>
    <form
      action="/user/register"
      method="post"
      class="max-sm:overflow-x-hidden"
    >
      <input
        required
        type="text"
        name="name"
        placeholder="Full Name"
        class="block p-[.8vmax] w-[100%] outline-none mt-[1.5vmax] rounded-md placeholder:text-zinc-500 max-sm:py-[1.5vmax] max-sm:px-[1.3vmax] max-sm:text-[2vmax] max-sm:mt-[2vmax]"
      />
      <input
        required
        type="text"
        name="username"
        placeholder="Username"
        class="block p-[.8vmax] w-[100%] outline-none mt-[1vmax] rounded-md placeholder:text-zinc-500 max-sm:py-[1.5vmax] max-sm:px-[1.3vmax] max-sm:text-[2vmax] max-sm:mt-[1.5vmax]"
      />
      <div class="flex mt-[1vmax] max-sm:mt-[1.5vmax]">
        <select
          required
          name="day"
          class="block p-[.8vmax] w-[32%] outline-none rounded-md placeholder:text-zinc-500 mr-[2%] max-sm:py-[1.5vmax] max-sm:px-[.8vmax] max-sm:text-[2vmax]"
        >
          <option value="" disabled selected>Day</option>
          <% for (let i = 1; i <= 31; i++) { %>
          <option value="<%= i %>"><%= i %></option>
          <% } %>
        </select>
        <select
          name="month"
          class="block p-[.8vmax] w-[32%] outline-none rounded-md placeholder:text-zinc-500 mr-[2%] max-sm:py-[1.5vmax] max-sm:px-[.8vmax] max-sm:text-[2vmax]"
        >
          <option value="" disabled selected>Month</option>
          <% for (let i = 1; i <= 12; i++) { %>
          <option value="<%= i %>"><%= i %></option>
          <% } %>
        </select>
        <select
          name="year"
          class="block p-[.8vmax] w-[32%] outline-none rounded-md placeholder:text-zinc-500 max-sm:py-[1.5vmax] max-sm:px-[.8vmax] max-sm:text-[2vmax]"
        >
          <option value="" disabled selected>Year</option>
          <% for (let i = 2024; i >= 1924; i--) { %>
          <option value="<%= i %>"><%= i %></option>
          <% } %>
        </select>
      </div>
      <div class="mt-[1vmax] max-sm:mt-[1.5vmax]">
        <label class="text-white">Gender:</label><br />
        <div class="flex mt-1">
          <input
            type="radio"
            id="male"
            name="gender"
            value="Male"
            class="mr-2 max-sm:mr-[.5vmax]"
            required
          /><label class="mr-8 max-sm:mr-[1.7vmax] text-white" for="male"
            >Male</label
          >
          <input
            type="radio"
            id="female"
            name="gender"
            value="Female"
            class="mr-2 max-sm:mr-[.5vmax]"
            required
          /><label class="mr-8 max-sm:mr-[1.7vmax] text-white" for="female"
            >Female</label
