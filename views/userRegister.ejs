<script>
  const cities = {
    AP: ["Visakhapatnam", "Vijayawada", "Guntur"],
    AR: ["Itanagar", "Naharlagun"],
    AS: ["Guwahati", "Dispur", "Jorhat"],
    BR: ["Patna", "Gaya", "Bhagalpur"],
    CT: ["Raipur", "Bhilai", "Korba"],
    GO: ["Panaji", "Margao"],
    GJ: ["Ahmedabad", "Vadodara", "Surat"],
    HR: ["Chandigarh", "Faridabad", "Gurugram"],
    HP: ["Shimla", "Dharamshala"],
    JK: ["Srinagar", "Jammu"],
    JH: ["Ranchi", "Jamshedpur"],
    KA: ["Bengaluru", "Mysuru", "Hubli"],
    KL: ["Thiruvananthapuram", "Kochi", "Kozhikode"],
    MP: ["Bhopal", "Indore", "Gwalior"],
    MH: ["Mumbai", "Pune", "Nagpur"],
    MN: ["Imphal"],
    ME: ["Shillong"],
    MZ: ["Aizawl"],
    NL: ["Kohima"],
    OD: ["Bhubaneswar", "Cuttack"],
    PB: ["Chandigarh", "Amritsar", "Ludhiana"],
    RJ: ["Jaipur", "Udaipur", "Jodhpur"],
    SK: ["Gangtok"],
    TN: ["Chennai", "Coimbatore", "Madurai"],
    TS: ["Hyderabad", "Warangal"],
    TR: ["Agartala"],
    UP: ["Lucknow", "Kanpur", "Agra"],
    UK: ["Dehradun", "Haridwar"],
    WB: ["Kolkata", "Siliguri", "Durgapur"],
  };

  document.querySelector(".state").addEventListener("change", function () {
    const state = this.value;
    const citySelect = document.querySelector(".city");
    citySelect.innerHTML =
      '<option value="" disabled selected>Select City</option>';

    if (state && cities[state]) {
      cities[state].forEach((city) => {
        const option = document.createElement("option");
        option.value = city;
        option.textContent = city;
        citySelect.appendChild(option);
      });
    }
  });
</script>
